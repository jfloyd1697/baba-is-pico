function _init()
    to={} -- objects and corresponding texts
 
    tf={} -- adjectives and their corresponding functions

    tf[TEXT.YOU]=you
    tf[TEXT.PUSH]=push
    tf[TEXT.STOP]=stp
    tf[TEXT.BLUE]=blue
    tf[TEXT.PULL]=pull
    tf[TEXT.MOVE]=_move
 
    items={}
    lw=16 --level width
    lh=16 --level height
 
    for x=0,lw-1 do --this scans the level
        for y=0,lh-1 do
            tle=mget(x,y)
            if(tle~=0) add(items,{x=x,y=y,o=tle,c=-1, dirx=-1, diry=0})
        end
    end
    music(0)
    -- tick()
end
