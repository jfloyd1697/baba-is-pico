function _draw()
    cls()
    --draw objects
    for o in all(items) do
        pal()
        if(o.c~=-1) for l=0,15 do pal(l,o.c) end
        spr(o.o, o.x*8, o.y*8)
    end
    pal()
    --draw rules
    for i,r in pairs(rles) do
        sub, pred = r[1], r[2]
        spr(sub,   1,  i*9+65)
        spr(TEXT.IS, 9,  i*9+65)
        spr(pred,   17, i*9+65)
    end
    for m in all(message) do
        print(m)
    end
end
