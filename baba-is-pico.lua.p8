function is_trans(r)
    sub, pred = r[1], r[2]
    return is_obj({o=sub}) and is_obj({o=pred})
end

function tick()
    for i,o in pairs(items) do
        for r in all(rles) do
            evaluate_rule(o,r)
        end
    end
    for i,o in pairs(items) do
        
        for r in all(rles) do
            sub, pred = r[1], r[2]
            if not is_text(o) then
                if is_trans(r) and o.o+1 == sub then
                    o.o = pred-1
                    goto continue
                end            
            end
            ::continue::
        end
        
        for j,o2 in pairs(items) do
            if o.x == o2.x and o.y == o2.y then
                if is_you(o) and is_defeat(o2) then
                    defeat(o)
                elseif is_you(o) and is_win(o2) then
                    win(o)
                elseif is_open(o) and is_shut(o2) then
                    defeat(o2)
                    defeat(o)
                elseif is_hot(o) and is_melt(o2) then
                    defeat(o2)
                end
            end
            ::next::
        end
    end   
end
