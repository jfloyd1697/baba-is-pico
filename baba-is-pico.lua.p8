function hr(o)
    if is_text(o) then return FLAGS.LOGIC
    else return o.o end
end


function is_text(o)
    return fget(o.o,4)
end

function is_obj(o)
    return fget(o.o,5)
end

function is_adj(o)
    return fget(o.o,6)
end

function is_logic(o)
    return fget(o.o,7)
end

function is_push(o)
    return psh[hr(o)]
end

function is_stop(o)
    return st[hr(o)]
end

function is_pull(o)
    return pll[hr(o)]
end

function is_move(o)
    return mve[hr(o)]
end

function is_defeat(o)
    return dft[hr(o)]
end

function is_you(o)
    return yu[hr(o)]
end

function is_win(o)
    return wn[hr(o)]
end

function is_open(o)
    return opn[hr(o)]
end

function is_shut(o)
    return sht[hr(o)]
end

function is_hot(o)
    return hot[hr(o)]
end

function is_melt(o)
    return melt[hr(o)]
end

function will_collide(o1, o2, dx, dy)
    return (o1.x+dx)==o2.x and (o1.y+dy)==o2.y
end

function atpos(os,x,y)
    ret={}
    for o in all(os) do
        if o.x==x and o.y==y then add(ret,o) end
    end
    return ret
end

function rulescn()
    local obj={}
    local is={}
    local adj={}
    for i,o in pairs(items) do
        if(is_obj(o)) add(obj,o)
        if(is_logic(o)) add(is ,o)
        if(is_adj(o)) add(adj,o)
    end
    rles={
        {TEXT.TEXT,TEXT.PUSH},
    }
    for i,o in pairs(items) do
        if is_text(o) and is_obj(o) then
            --horizontal
            for o2 in all(atpos(is,o.x+1,o.y)) do
                for o3 in all(atpos(adj,o.x+2,o.y)) do
                    add(rles,{o.o,o3.o})
                end
                for o3 in all(atpos(obj,o.x+2,o.y)) do
                    add(rles,{o.o,o3.o})
                end
            end
            --vertical
            for o2 in all(atpos(is,o.x,o.y+1)) do
                for o3 in all(atpos(adj,o.x,o.y+2)) do
                    add(rles,{o.o,o3.o})
                end
                for o3 in all(atpos(obj,o.x,o.y+2)) do
                    add(rles,{o.o,o3.o})
                end
            end
        end
    end
end
