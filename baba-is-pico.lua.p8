function _update()
    for i,o in pairs(items) do
        o.c=-1
    end
    rulescn()
    
    for i in all({ðŸ…¾ï¸,â¬‡ï¸,â¬†ï¸,â¬…ï¸,âž¡ï¸}) do
        if btnp(i) then
            tick()
        end
    end
