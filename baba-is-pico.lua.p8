function defeat(self)
    print("you_lose")
    sfx(TEXT.DEFEAT)
    del(items, self)
end

function _move(self)
    if not check_move(self, self.dirx, 0) then
        self.dirx = -self.dirx
    end
    if not check_move(self, 0, self.diry) then
        self.diry = -self.diry
    end
    move(self, self.dirx, self.diry)
end

function is_out_of_bounds(x, y)
    return x < 0 or x > lw-1 or y < 0 or y > lh-1
end

function check_move(self, mx, my)
    st={}
    psh={}
    pll={}
    mve={}
    dft={}
    yu={}
    wn={}
    opn={}
    sht={}
    hot={}
    melt={}
    local chk=true

    q = {
        [TEXT.PUSH] = psh,
        [TEXT.STOP] = st,
        [TEXT.PULL] = pll,
        [TEXT.MOVE] = mve,
        [TEXT.DEFEAT] = dft,
        [TEXT.YOU] = yu,
        [TEXT.WIN] = wn,
        [TEXT.OPEN] = opn,
        [TEXT.SHUT] = sht,
        [TEXT.HOT] = hot,
        [TEXT.MELT] = melt,
    }

    local eval = function(sub, pred, txt, tbl)
        if pred == txt then
            tbl[sub-1] = true
        end
    end

    for r in all(rles) do
        sub, pred = r[1], r[2]
        for txt, tbl in pairs(q) do
            eval(sub, pred, txt, tbl)
        end
    end

    for other in all(items) do
        if other == self then
            goto continue
        end
        if will_collide(self, other, mx, my) then
            if is_stop(other) or (is_shut(other) and not is_open(self)) then
                chk=false
            elseif is_push(other) and not is_pull(self) then
                chk = move(other,mx,my)
            end
        elseif is_pull(other) and not is_push(self) and will_collide(other, self, mx, my) then
            chk = move(other, mx, my)
        end
        ::continue::
    end
    if is_out_of_bounds(self.x+mx, self.y+my) then
        chk = false
    end

    return chk
end

function move(self,mx,my)

    local chk = check_move(self, mx, my)
    
    if chk then
        self.x+=mx
        self.y+=my
        if mx > 0 then
            self.dirx = 1
            self.diry = 0
        elseif mx < 0 then
            self.dirx = -1
            self.diry = 0
        elseif my > 0 then
            self.dirx = 0
            self.diry = 1
        elseif my < 0 then
            self.dirx = 0
            self.diry = -1
        end        
        sfx(rnd({60, 61, 62, 63}))
    end
    return chk
end
